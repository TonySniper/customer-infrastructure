{
    "$schema": "https://schema.management.azure.com/schemas/2018-05-01/subscriptionDeploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "templateBaseUrl": {
            "type": "string"
        },
        "primaryLocation": {
            "type": "string"
        },
        "secondaryLocation": {
            "type": "string"
        },
        "primaryLocationShortName": {
            "type": "string"
        },
        "secondaryLocationShortName": {
            "type": "string"
        },
        "appName": {
            "type": "string"
        }
    },
    "functions": [],
    "variables": {
        "primaryRgName": "[concat('rg-', parameters('appName'), '-', parameters('primaryLocationShortName'))]",
        "secondaryRgName": "[concat('rg-', parameters('appName'), '-', parameters('secondaryLocationShortName'))]",


        "rgTemplateUrl": "[uri(parameters('templateBaseUrl'), 'nested/ResourceGroup/ResourceGroup.json')]" 
    },
    "resources": [
        
    ],
    "outputs": {}
}