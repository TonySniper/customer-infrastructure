{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
         "environment": {
            "type": "string"
        },
        "appName": {
            "type": "string"
        },
        "templateBaseUrl": {
            "type": "string",
            "metadata": {
                "description": "Base URL for the linked arm template files"
            }
        }
    },
    "functions": [],
    "variables": {        
    },
    "resources": [
        {
            "name": "keyVaultDeployment",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('templateBaseUrl'), '/composed/Storage/DeployKeyVault.json')]",
                    "contentVersion": "1.0.0.0"
                },
                "parametersLink": {
                    "uri": "[concat(parameters('templateBaseUrl'), '/composed/Storage/Parameters/DeployKeyVault.parameters.', parameters('environment'), '.json')]"
                }
            }
        },
        {
            "name": "mysqlServerDeployment",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2019-10-01",
            "dependsOn": [
                "keyVaultDeployment"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[concat(parameters('templateBaseUrl'), '/composed/Storage/DeployMySql.json')]",
                    "contentVersion": "1.0.0.0"
                },
                "parametersLink": {
                    "uri": "[concat(parameters('templateBaseUrl'), '/composed/Storage/Parameters/DeployMySql.parameters.wow.', parameters('environment'), '.json')]"
                }
            }
        }
    ],
    "outputs": {}
}